<h1><%= question_header(@question) %></h1>
<%= form_with model: [@test, @question] do |form| %>
  <%= render 'shared/errors', resource: @question %>

  <p>
    <%= form.label :body %><br>
    <%= form.text_field :body %>
  </p><br>

  <p><%= form.submit %></p>

<% end %>
<%= link_to 'Back', tests_path %>
